<%= form_with model:@student do |form| %>

<div class="field" >
	<%= form.label :name %><br>
	<%= form.text_field :name %>
</div><br>

<div class="field" >
   <%= form.label :contact %><br>
   <%= form.text_field :contact %>
</div><br>

<div class="field" >
   <%= form.label :email %><br>
   <%= form.text_field :email %>
</div><br>	

<div class="field" > 
    <%= form.label :address %><br>
    <%= form.text_field :address %>
</div><br>

<div class="actions">
    <%= form.submit %>
</div>
<% end %> 	
