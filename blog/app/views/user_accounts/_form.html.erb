<%= form_with model: [@user_account] do |form| %>

<div class="field">
	<%= form.label :name %>
	<%= form.text_field :name %>
</div>

<div class="field">
    <%= form.label :contact %>
    <%= form.text_field :contact %>
</div>

<div class="field">
	<%= form.label :email %>
	<%= form.text_field :email %>
</div>    	

<div class="actions">
	<%= form.submit %>
</div>

<% end %>	
