<h2>supplier Data </h2>

<table width="100%", border="1px";>
	<thead>
		<tr>
			<th>name</th>
			<th>show</th>
			<th>edit</th>
			<th>destroy</th>
        </tr>
    </thead>
    <tbody>
    		<% @suppliers.each do |supplier| %>
    	<tr>
    		<td><%= supplier.name %></td>
    		<td><%= link_to "show", supplier_path(supplier.id) %></td>
    		<td><%= link_to "edit", edit_supplier_path(supplier.id) %></td>
    		<td><%= link_to "destroy", supplier , data: { method: :delete,
    			confirm: "are you sure"
    		} %></td>
        </tr>		
    		<% end %>
    </tbody>
</table>   

    <%= link_to "new supplier data", new_supplier_path %>