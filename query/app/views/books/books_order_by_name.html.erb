
<h2>Books in order</h2>

<table width="100%", border="1px";>
	<thead>
		<tr>
			<th>title</th>
			<th>year of published</th>
			<th>price</th>
			<th>out of print</th>
			<th>views</th>
        </tr>
    </thead>
    <tbody>
    		<% @books.each do |book| %>
    	<tr>
    		<td><%= book.title %></td>
    		<td><%= book.year_of_published %></td>
    		<td><%= book.price %></td>
    		<td><%= book.out_of_print %></td>
    		<td><%= book.views %></td>
        </tr>		
    		<% end %>
    </tbody>
</table>  