<%= form_with model:@review do |form| %>

<p>
	<%= form.label :title %>
	<%= form.text_field :title %>
</p>	

<p>
	<%= form.label :body %>
	<%= form.text_field :body %>
</p>

<p>
	<%= form.label :rating %>
	<%= form.text_field :rating %>
</p>

<p>
	<%= form.label :state %>
	<%= form.text_field :state %>
</p>

<p>
	<%= form.label :customer_id %>
	<%= form.text_field :book_id %>
</p>

<p>
	<%= form.submit %>
</p>

<% end %>